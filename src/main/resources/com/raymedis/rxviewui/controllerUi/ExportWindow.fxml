<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXCheckBox?>
<?import com.raymedis.rxviewui.ViewBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<GridPane id="GridPane" fx:id="rootNode" alignment="CENTER" maxHeight="500.0" maxWidth="800.0" minHeight="500.0" minWidth="800.0" prefHeight="500.0" prefWidth="800.0" styleClass="fontStyle" stylesheets="@../../../../Styles/Css/ExportWindow.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.raymedis.rxviewui.controller.ExportWindow">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <ViewBox fx:id="viewBox" id="gridPane2" alignment="CENTER" maxHeight="500.0" maxWidth="800.0" minHeight="500.0" minWidth="800.0" prefHeight="500.0" prefWidth="800.0">
      <VBox alignment="CENTER" maxHeight="500.0" maxWidth="800.0" minHeight="500.0" minWidth="800.0" prefHeight="500.0" prefWidth="800.0" spacing="25.0">
         <children>
            <HBox fx:id="imageFormat" alignment="CENTER_LEFT" maxHeight="60.0" maxWidth="800.0" minHeight="60.0" minWidth="800.0" prefHeight="60.0" prefWidth="800.0" spacing="15.0">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <children>
                  <Label maxHeight="30.0" maxWidth="175.0" minHeight="30.0" minWidth="175.0" prefHeight="30.0" prefWidth="175.0" text="Image Format">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Label>
                  <Label fx:id="imageFormatMandatory" maxHeight="50.0" maxWidth="25.0" minHeight="50.0" minWidth="25.0" prefHeight="50.0" prefWidth="25.0" text=" * ">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                     <padding>
                        <Insets left="-25.0" />
                     </padding>
                  </Label>
                  <JFXButton id="unSelectedButton" fx:id="dicomButton" maxHeight="40.0" maxWidth="100.0" minHeight="40.0" minWidth="100.0" onAction="#dicomClick" prefHeight="40.0" prefWidth="100.0" text="DICOM" textFill="WHITE">
                     <font>
                        <Font name="Century Gothic" size="16.0" />
                     </font>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </JFXButton>
                  <JFXButton id="unSelectedButton" fx:id="bmpButton" maxHeight="40.0" maxWidth="100.0" minHeight="40.0" minWidth="100.0" onAction="#bmpClick" prefHeight="40.0" prefWidth="100.0" text="BMP" textFill="WHITE">
                     <font>
                        <Font name="Century Gothic" size="16.0" />
                     </font>
                  </JFXButton>
                  <JFXButton id="unSelectedButton" fx:id="pngButton" maxHeight="40.0" maxWidth="100.0" minHeight="40.0" minWidth="100.0" onAction="#pngClick" prefHeight="40.0" prefWidth="100.0" text="PNG" textFill="WHITE">
                     <font>
                        <Font name="Century Gothic" size="16.0" />
                     </font>
                  </JFXButton>
                  <JFXButton id="unSelectedButton" fx:id="jpegButton" maxHeight="40.0" maxWidth="100.0" minHeight="40.0" minWidth="100.0" onAction="#jpegClick" prefHeight="40.0" prefWidth="100.0" text="JPEG" textFill="WHITE">
                     <font>
                        <Font name="Century Gothic" size="16.0" />
                     </font>
                  </JFXButton>
               </children>
               <padding>
                  <Insets left="50.0" />
               </padding>
            </HBox>
            <HBox fx:id="exportPathBox" alignment="CENTER_LEFT" maxHeight="50.0" maxWidth="800.0" minHeight="50.0" minWidth="800.0" prefHeight="50.0" prefWidth="800.0" spacing="15.0">
               <children>
                  <Label maxHeight="30.0" maxWidth="135.0" minHeight="30.0" minWidth="135.0" prefHeight="30.0" prefWidth="135.0" text="Export Path">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Label>
                  <Label fx:id="exportMandatory" maxHeight="50.0" maxWidth="25.0" minHeight="50.0" minWidth="25.0" prefHeight="50.0" prefWidth="25.0" text=" * ">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="-25.0" top="-10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField id="TextField" fx:id="selectedPath" editable="false" maxHeight="40.0" maxWidth="375.0" minHeight="40.0" minWidth="375.0" prefHeight="40.0" prefWidth="375.0">
                     <font>
                        <Font name="Century Gothic" size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="65.0" />
                     </HBox.margin>
                  </TextField>
                  <JFXButton id="ActionButton" fx:id="pathButton" maxHeight="40.0" maxWidth="50.0" minHeight="40.0" minWidth="50.0" onAction="#dialogBoxClick" prefHeight="40.0" prefWidth="50.0" text="..." textFill="WHITE">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </JFXButton>
               </children>
               <padding>
                  <Insets left="50.0" />
               </padding>
            </HBox>
            <HBox maxHeight="50.0" maxWidth="800.0" minHeight="50.0" minWidth="800.0" prefHeight="50.0" prefWidth="800.0" spacing="20.0">
               <children>
                  <Label maxHeight="50.0" maxWidth="200.0" minHeight="50.0" minWidth="200.0" prefHeight="50.0" prefWidth="200.0" text="Burning Options">
                     <font>
                        <Font name="Century Gothic" size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Label>
                  <JFXCheckBox fx:id="burnAnnotationsCheckBox" maxHeight="50.0" maxWidth="175.0" minHeight="50.0" minWidth="175.0" prefHeight="50.0" prefWidth="175.0" text="Annotations">
                     <font>
                        <Font name="Century Gothic" size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </JFXCheckBox>
                  <JFXCheckBox fx:id="burnInformationCheckBox" maxHeight="50.0" maxWidth="175.0" minHeight="50.0" minWidth="175.0" prefHeight="50.0" prefWidth="175.0" text="Information">
                     <font>
                        <Font name="Century Gothic" size="18.0" />
                     </font>
                  </JFXCheckBox>
                  <JFXCheckBox fx:id="burnCropCheckBox" maxHeight="50.0" maxWidth="100.0" minHeight="50.0" minWidth="100.0" prefHeight="50.0" prefWidth="100.0" text="Crop">
                     <font>
                        <Font name="Century Gothic" size="18.0" />
                     </font>
                  </JFXCheckBox>
               </children>
               <padding>
                  <Insets left="50.0" />
               </padding>
            </HBox>
            <HBox spacing="30.0">
               <children>
                  <Label maxHeight="50.0" maxWidth="200.0" minHeight="50.0" minWidth="200.0" prefHeight="50.0" prefWidth="200.0" text="Current State ">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="statusLabel" maxHeight="50.0" maxWidth="250.0" minHeight="50.0" minWidth="250.0" prefHeight="50.0" prefWidth="250.0" text="Ready">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Label>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets left="50.0" />
               </padding>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <VBox alignment="CENTER" maxHeight="30.0" maxWidth="800.0" minHeight="30.0" minWidth="800.0" prefHeight="30.0" prefWidth="800.0">
               <children>
                  <ProgressBar fx:id="progressBar" disable="true" maxHeight="9.0" maxWidth="500.0" minHeight="9.0" minWidth="500.0" prefHeight="9.0" prefWidth="500.0" />
               </children>
            </VBox>
            <HBox alignment="CENTER" maxHeight="50.0" maxWidth="800.0" minHeight="50.0" minWidth="800.0" prefHeight="50.0" prefWidth="800.0" spacing="100.0">
               <children>
                  <JFXButton id="ActionButton" fx:id="startButton" maxHeight="50.0" maxWidth="125.0" minHeight="50.0" minWidth="125.0" onAction="#startExport" prefHeight="50.0" prefWidth="125.0" text="Start" textFill="WHITE">
                     <font>
                        <Font name="Century Gothic" size="18.0" />
                     </font>
                  </JFXButton>
                  <JFXButton id="ActionButton" fx:id="closeButton" maxHeight="50.0" maxWidth="125.0" minHeight="50.0" minWidth="125.0" onAction="#closeClick" prefHeight="50.0" prefWidth="125.0" text="Close" textFill="WHITE">
                     <font>
                        <Font name="Century Gothic" size="18.0" />
                     </font>
                  </JFXButton>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
      <columnConstraints>
         <ColumnConstraints />
      </columnConstraints>
      <rowConstraints>
         <RowConstraints />
      </rowConstraints>
    </ViewBox>
</GridPane>
