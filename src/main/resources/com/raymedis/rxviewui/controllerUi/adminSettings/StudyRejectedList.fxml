<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<GridPane alignment="CENTER" maxHeight="1030.0" maxWidth="1620.0" minHeight="1030.0"
          minWidth="1620.0" prefHeight="1030.0" prefWidth="1620.0"
          stylesheets="@../../../../../Styles/Css/AdminSettings/Study/StudyRejectedList.css"
          xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.raymedis.rxviewui.controller.adminSettings.study.StudyRejectedList">
    <VBox alignment="CENTER" maxHeight="1030.0" maxWidth="1620.0" minHeight="1030.0" minWidth="1620.0" prefHeight="1030.0" prefWidth="1620.0" spacing="100.0">
        <children>
            <VBox id="ShadowEffectBox2" maxHeight="350.0" maxWidth="1100.0" minHeight="350.0" minWidth="1100.0" prefHeight="350.0" prefWidth="1100.0">
                <children>
                    <HBox alignment="CENTER_LEFT" maxHeight="60.0" maxWidth="1250.0" minHeight="60.0" minWidth="1250.0" prefHeight="60.0" prefWidth="1250.0">
                        <children>
                            <Label id="HeaderLabel2" alignment="CENTER" maxHeight="60.0" maxWidth="350.0" minHeight="60.0" minWidth="350.0" prefHeight="60.0" prefWidth="350.0" styleClass="fontStyle" text="Filter &amp; Search" textAlignment="CENTER" textFill="WHITE" wrapText="true">
                                <font>
                                    <Font name="Century Gothic" size="22.0" />
                                </font>
                            </Label>
                        </children>
                        <padding>
                            <Insets left="25.0" top="-60.0" />
                        </padding>
                    </HBox>
               <VBox spacing="40.0">
                  <children>
                     <HBox spacing="25.0">
                        <children>
                           <JFXComboBox fx:id="searchByComboBox" focusColor="#575656" maxHeight="50.0" maxWidth="225.0" minHeight="50.0" minWidth="225.0" onAction="#selectionChanged" prefHeight="50.0" prefWidth="225.0" unFocusColor="#575656" value="None">
                              <padding>
                                 <Insets left="5.0" />
                              </padding>
                              <items>
                                 <FXCollections fx:factory="observableArrayList">
                                    <String fx:value="None" />
                                    <String fx:value="Patient name" />
                                    <String fx:value="Patient Id" />
                                    <String fx:value="Sex" />
                                    <String fx:value="Accession No." />
                                    <String fx:value="Referring Physician" />
                                 </FXCollections>
                              </items>
                           </JFXComboBox>
                           <TextField id="textField" fx:id="detailsText" maxHeight="50.0" maxWidth="225.0" minHeight="50.0" minWidth="225.0" prefHeight="50.0" prefWidth="225.0" promptText="Enter Detail" styleClass="fontStyle">
                              <font>
                                 <Font name="Century Gothic" size="12.0" />
                              </font>
                           </TextField>
                        </children>
                        <VBox.margin>
                           <Insets left="25.0" />
                        </VBox.margin>
                     </HBox>
                     <HBox spacing="50.0">
                        <children>
                           <JFXButton id="ActionButton" fx:id="todayButton" maxHeight="50.0" maxWidth="175.0" minHeight="50.0" minWidth="175.0" onAction="#todayBtnClick" prefHeight="50.0" prefWidth="175.0" styleClass="fontStyle" text="Today" textFill="WHITE">
                              <font>
                                 <Font name="Century Gothic" size="26.0" />
                              </font>
                           </JFXButton>
                           <JFXButton id="ActionButton" fx:id="weekButton" maxHeight="50.0" maxWidth="175.0" minHeight="50.0" minWidth="175.0" onAction="#weekBtnClick" prefHeight="50.0" prefWidth="175.0" styleClass="fontStyle" text="Week" textFill="WHITE">
                              <font>
                                 <Font name="Century Gothic" size="26.0" />
                              </font>
                           </JFXButton>
                           <JFXButton id="ActionButton" fx:id="monthButton" maxHeight="50.0" maxWidth="175.0" minHeight="50.0" minWidth="175.0" onAction="#monthBtnClick" prefHeight="50.0" prefWidth="175.0" styleClass="fontStyle" text="Month" textFill="WHITE">
                              <font>
                                 <Font name="Century Gothic" size="26.0" />
                              </font>
                           </JFXButton>
                        </children>
                        <VBox.margin>
                           <Insets left="25.0" />
                        </VBox.margin>
                     </HBox>
                     <HBox spacing="50.0">
                        <children>
                           <DatePicker fx:id="fromDate" editable="false" maxHeight="50.0" maxWidth="225.0" minHeight="50.0" minWidth="225.0" prefHeight="50.0" prefWidth="225.0" promptText="From" />
                           <DatePicker fx:id="toDate" editable="false" prefHeight="50.0" prefWidth="225.0" promptText="To" />
                           <JFXButton id="ActionButton" fx:id="searchButton" maxHeight="50.0" maxWidth="175.0" minHeight="50.0" minWidth="175.0" onAction="#searchClick" prefHeight="50.0" prefWidth="175.0" styleClass="fontStyle" text="Search" textFill="WHITE">
                              <font>
                                 <Font name="Century Gothic" size="26.0" />
                              </font>
                           </JFXButton>
                           <JFXButton id="ActionButton" fx:id="clearButton" maxHeight="50.0" maxWidth="175.0" minHeight="50.0" minWidth="175.0" onAction="#clearClick" prefHeight="50.0" prefWidth="175.0" styleClass="fontStyle" text="Clear" textFill="WHITE">
                              <font>
                                 <Font name="Century Gothic" size="26.0" />
                              </font>
                           </JFXButton>
                        </children>
                        <VBox.margin>
                           <Insets left="25.0" />
                        </VBox.margin>
                     </HBox>
                  </children>
               </VBox>
                </children>
            </VBox>
         <VBox id="ShadowEffectBox2" alignment="CENTER" maxHeight="450.0" maxWidth="1100.0" minHeight="450.0" minWidth="1100.0" prefHeight="450.0" prefWidth="1100.0">
            <children>
               <HBox alignment="CENTER_LEFT" maxHeight="60.0" maxWidth="1100.0" minHeight="60.0" minWidth="1100.0" prefHeight="60.0" prefWidth="1100.0">
                  <children>
                     <Label id="HeaderLabel2" alignment="CENTER" maxHeight="60.0" maxWidth="250.0" minHeight="60.0" minWidth="250.0" prefHeight="60.0" prefWidth="250.0" styleClass="fontStyle" text="Search Result" textAlignment="CENTER" textFill="WHITE" wrapText="true">
                        <font>
                           <Font name="Century Gothic" size="22.0" />
                        </font>
                     </Label>
                  </children>
                  <padding>
                     <Insets left="25.0" top="-100.0" />
                  </padding>
               </HBox>
               <HBox alignment="CENTER" maxHeight="350.0" maxWidth="1100.0" minHeight="350.0" minWidth="1100.0" prefHeight="350.0" prefWidth="1100.0">
                  <children>
                     <HBox alignment="CENTER" maxHeight="300.0" maxWidth="1000.0" minHeight="300.0" minWidth="1000.0" prefHeight="300.0" prefWidth="1000.0">
                        <children>
                           <ScrollPane hbarPolicy="NEVER" maxHeight="300.0" maxWidth="1000.0" minHeight="300.0" minWidth="1000.0" prefHeight="300.0" prefWidth="1000.0" vbarPolicy="NEVER">
                              <content>
                                 <TableView fx:id="rejectedListDataGrid" focusTraversable="false" maxHeight="300.0" maxWidth="1000.0" minHeight="300.0" minWidth="1000.0" prefHeight="300.0" prefWidth="1000.0">
                                    <placeholder>
                                       <Pane />
                                    </placeholder>
                                    <columns>
                                       <TableColumn fx:id="patientIdColumn" maxWidth="200.0" minWidth="200.0" prefWidth="200.0" text="Patient ID" />
                                       <TableColumn fx:id="patientNameColumn" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" text="Patient Name" />
                                       <TableColumn fx:id="dobColumn" maxWidth="200.0" minWidth="200.0" prefWidth="200.0" text="DOB" />
                                       <TableColumn fx:id="sexColumn" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" text="Sex" />
                                       <TableColumn fx:id="studyDateTimeColumn" maxWidth="200.0" minWidth="200.0" prefWidth="200.0" text="Study Date Time" />
                                    </columns>
                                    <columnResizePolicy>
                                       <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                    </columnResizePolicy>
                                 </TableView>
                              </content>
                           </ScrollPane>
                        </children>
                     </HBox>
                  </children>
                  <VBox.margin>
                     <Insets />
                  </VBox.margin>
                  <padding>
                     <Insets top="-100.0" />
                  </padding>
               </HBox>
               <HBox alignment="CENTER_RIGHT" maxHeight="50.0" maxWidth="1100.0" minHeight="50.0" minWidth="1100.0" prefHeight="50.0" prefWidth="1100.0">
                  <children>
                     <JFXButton id="ActionButton" fx:id="deleteClick" maxHeight="50.0" maxWidth="175.0" minHeight="50.0" minWidth="175.0" onAction="#deleteClick" prefHeight="50.0" prefWidth="175.0" styleClass="fontStyle" text="Delete" textFill="WHITE">
                        <font>
                           <Font name="Century Gothic" size="26.0" />
                        </font>
                     </JFXButton>
                  </children>
                  <VBox.margin>
                     <Insets right="50.0" top="-50.0" />
                  </VBox.margin>
               </HBox>
            </children>
         </VBox>
        </children>
        <GridPane.margin>
            <Insets />
        </GridPane.margin>
    </VBox>
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <styleClass>
        <String fx:value="GridPane" />
        <String fx:value="fontStyle" />
    </styleClass>
</GridPane>
