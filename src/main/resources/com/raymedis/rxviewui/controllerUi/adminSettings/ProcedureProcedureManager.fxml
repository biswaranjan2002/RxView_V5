<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import java.lang.String?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<GridPane maxHeight="1030.0" maxWidth="1620.0" minHeight="1030.0" minWidth="1620.0" prefHeight="1030.0" prefWidth="1620.0" styleClass="GridPane"
          stylesheets="@../../../../../Styles/Css/AdminSettings/ProcedureManager.css" xmlns="http://javafx.com/javafx/21"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.raymedis.rxviewui.controller.adminSettings.procedureManager.ProcedureProcedureManager">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
   <children>
      <VBox maxHeight="1030.0" maxWidth="1620.0" minHeight="1030.0" minWidth="1620.0" prefHeight="1030.0" prefWidth="1620.0">
         <children>
            <HBox alignment="CENTER_LEFT" maxHeight="60.0" maxWidth="1620.0" minHeight="60.0" minWidth="1620.0" prefHeight="60.0" prefWidth="1620.0">
               <children>
                  <JFXButton fx:id="categoryButton" onAction="#categoryClick" maxHeight="50.0" maxWidth="275.0" minHeight="50.0" minWidth="275.0" prefHeight="50.0" prefWidth="275.0" text="Category">
                     <styleClass>
                        <String fx:value="fontStyle" />
                        <String fx:value="selectedButton" />
                     </styleClass></JFXButton>
                  <JFXButton fx:id="procedureButton" onAction="#procedureClick" maxHeight="50.0" maxWidth="275.0" minHeight="50.0" minWidth="275.0" prefHeight="50.0" prefWidth="275.0" text="Procedure">
                     <styleClass>
                        <String fx:value="fontStyle" />
                        <String fx:value="unSelectedButton" />
                     </styleClass></JFXButton>
                  <JFXButton fx:id="stepButton" onAction="#stepClick"  maxHeight="50.0" maxWidth="275.0" minHeight="50.0" minWidth="275.0" prefHeight="50.0" prefWidth="275.0" text="Step">
                     <styleClass>
                        <String fx:value="fontStyle" />
                        <String fx:value="unSelectedButton" />
                     </styleClass></JFXButton>
                  <JFXButton fx:id="bodyPartProjectionButton" onAction="#bodyPartProjectionClick" maxHeight="50.0" maxWidth="275.0" minHeight="50.0" minWidth="275.0" prefHeight="50.0" prefWidth="275.0" text="Bodypart/Projection">
                     <styleClass>
                        <String fx:value="unSelectedButton" />
                        <String fx:value="fontStyle" />
                     </styleClass></JFXButton>
               </children>
            </HBox>
            <VBox fx:id="contentNode" maxHeight="970.0" maxWidth="1620.0" minHeight="970.0" minWidth="1620.0" prefHeight="970.0" prefWidth="1620.0">
<!--                <fx:include source="ProcedureManagerProjection.fxml" />-->
            </VBox>
         </children>
      </VBox>
   </children>
</GridPane>
